Options -MultiViews
RewriteEngine On

# RewriteBase depende del entorno:
# - En Railway/Producci√≥n: RewriteBase /
# - En XAMPP local: RewriteBase /GestionInventario/public/
RewriteBase /GestionInventario/public/

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
